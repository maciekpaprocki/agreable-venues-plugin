@import 'leaflet'

.venues__count
  z-index 100
  position absolute
  top 0
  right 50%
  transform translateY(-50%)
  display block
  width 50px
  height  50px
  background #ff00ff

.venues__map
  height 100%
  left 0
  position absolute
  top 0
  width 100%

.venues__search
  background rgba(255,255,255,0.8)
  left 20px
  padding 5px
  position absolute
  top 20px
  z-index 10

