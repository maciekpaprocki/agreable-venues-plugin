@import 'leaflet/leaflet'
@import 'leaflet/leaflet.cluster'

@import 'venues-overlay-container'
@import 'venues-marker'
@import 'venues-overlay'
@import 'venues-share'
@import 'venues-search'
@import 'venues-map'

body.agreable-venues-plugin
  height 100%
  overflow hidden

.venues
  position relative
  clearfix()

.venues__outdated
  background white
  height auto
  left 50%
  padding 20px
  position absolute
  top 50%
  transform translate(-50%, -50%)
  width 70%
  z-index 100

