@import 'nib'
@import 'allure-breakpoints'


.venues
  position relative
  clearfix()

.venues__count
  z-index 100
  position absolute
  top 0
  right 50%
  display block
  width 50px
  height  50px
  background #ff00ff

.venues__map
  zoom 1
  float left
  width 100%
  min-height 100%

.venues__links-test
  z-index 110
  display block
  position: absolute;
  top: 60px;
  left: 20px;
  padding: 10px;

.venues__venue
  position absolute
  right 300px
